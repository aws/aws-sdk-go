            "expect": {
                "endpoint": {
                    "properties": {
                        "authSchemes": [
                            {
                                "signingRegion": "us-gov-west-1",
                                "signingName": "support",
                                "name": "sigv4"
                            }
                        ]
                    },
                    "url": "https://support.us-gov-west-1.amazonaws.com"
                }
            },
            "params": {
                "UseFIPS": false,
